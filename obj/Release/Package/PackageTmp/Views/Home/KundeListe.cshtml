@model List<Pizza_Ukesoppgave.Models.Kunde>

@{
    ViewBag.Title = "Kunder";
}

<h2><i class='fas fa-users'></i> Kunder</h2>


<table class="table">
    <tr>
        <th>
       Navn
        </th>
        <th>
      Adresse
       </th>
     <th> Telefoner </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Navn
        </td>
        <td>
            @item.Adresse
        </td>
        <td>
            @item.Telefoner
        </td>
        <td>
          
    </tr>
}

</table>
